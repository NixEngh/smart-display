{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.59.20",
    "@tanstack/react-table": "npm:@tanstack/react-table@^8.20.5",
    "@types/react": "npm:@types/react@^18.3.11",
    "@types/react-dom": "npm:@types/react-dom@^18.3.1",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.3",
    "autoprefixer": "npm:autoprefixer@^10.4.20",
    "class-variance-authority": "npm:class-variance-authority@^0.7.0",
    "clsx": "npm:clsx@^2.1.1",
    "date-fns": "npm:date-fns@^4.1.0",
    "lucide-react": "npm:lucide-react@^0.456.0",
    "postcss": "npm:postcss@^8.4.47",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-icons": "npm:react-icons@^5.3.0",
    "tailwind-merge": "npm:tailwind-merge@^2.5.4",
    "tailwindcss": "npm:tailwindcss@^3.4.14",
    "tailwindcss-animate": "npm:tailwindcss-animate@^1.0.7",
    "vite": "npm:vite@^5.4.9"
  }
}
